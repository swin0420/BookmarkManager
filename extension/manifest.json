{
  "manifest_version": 3,
  "name": "Twitter Bookmarks Exporter",
  "version": "1.0.0",
  "description": "Export your Twitter/X bookmarks to your local bookmarks manager",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
